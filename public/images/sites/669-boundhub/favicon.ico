	filter: alpha(opacity=0.2);
}

.block-video .no-player .message {
	display:  block;
	position: absolute;
	text-align: center;
	padding: 20px;
	left: 0;
	right: 0;
	top: 30px;
	font-size: 20px;
	font-weight: bold;
	line-height: 25px;
	color: #9b9b9b;
}

.block-video .no-player .message em {
	font-size: 20px;
	font-weight: bold;
	line-height: 25px;
	color: #ffffff;
}

.block-video .no-player .message form {
